#!/usr/bin/python3
# tsting doctests

def factorial(n):
    """a dunction to find the factorial of a number
    """
    if (n == 0):
        return (1)
    if (n < 0):
        raise ValueError("value ust be above 0")
    return (factorial( n - 1))
if __name__ = "__main__":
    p = input("Enter the input")
    print(factorial(p))
