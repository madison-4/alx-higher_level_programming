#!/usr/bin/node
// a script to print multiple lamguages

const rep = process.argv[2];
const num = parseInt(rep);
let word = '';

if (isNaN(num)) {
    console.log('Missing size');
  process.exit();
} else {
    for (let i = 0; i < num; i++) {
	for (let j = 0; j < num; j++) {
	    word += 'X'
	}
	console.log(word);
  }
}
